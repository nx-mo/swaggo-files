{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "git-submodules": {
    "enabled": true
  },
  "packageRules": [
    {
      "matchDatasources": [
        "git-tags"
      ],
      "matchPackageNames": [
        "swagger-ui"
      ],
      "registryUrls": [
        "https://github.com/swagger-api/swagger-ui"
      ],
      "rangeStrategy": "pin",
      "commitMessage": "Update swagger-ui to latest tag",
      "prTitle": "Update swagger-ui to latest tag"
    }
  ],
  "prConcurrentLimit": 1,
  "prCreation": "not-pending"
}
